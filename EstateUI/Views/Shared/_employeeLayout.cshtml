@{
    Layout = null;
}
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <title>Emlakçı Yönetim Sistemi</title>
    <link href="~/AdminTemp/img/employer-icon.png" rel="icon" type="image/x-icon" />
    <link href="~/AdminTemp/css/bootstrap.min.css" rel="stylesheet">
    <link href="~/AdminTemp/font-awesome/css/font-awesome.css" rel="stylesheet">
    <link href="~/AdminTemp/css/animate.css" rel="stylesheet">
    <link href="~/AdminTemp/css/style.css" rel="stylesheet">
</head>

<body class="">
    <div id="wrapper">
        <nav class="navbar-default navbar-static-side" role="navigation">
            @await Html.PartialAsync("Employee/employeeLeftNavbar") @* Sol Navbar *@
        </nav>  
        <div id="page-wrapper" class="gray-bg">
            @await Html.PartialAsync("Employee/employeeHeader") @* Header *@
            <div class="wrapper wrapper-content">
                @RenderBody()
            </div>
            @await Html.PartialAsync("Employee/employeeFooter") @* Footer *@

        </div>
    </div>
    <!-- Mainly scripts -->
    <script src="~/AdminTemp/js/jquery-3.1.1.min.js"></script>
    <script src="~/AdminTemp/js/popper.min.js"></script>
    <script src="~/AdminTemp/js/bootstrap.js"></script>
    <script src="~/AdminTemp/js/plugins/metisMenu/jquery.metisMenu.js"></script>
    <script src="~/AdminTemp/js/plugins/slimscroll/jquery.slimscroll.min.js"></script>

    <!-- Custom and plugin javascript -->
    <script src="~/AdminTemp/js/inspinia.js"></script>
    <script src="~/AdminTemp/js/plugins/pace/pace.min.js"></script>
</body>
</html>

