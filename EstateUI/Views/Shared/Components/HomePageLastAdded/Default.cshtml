@model List<EstateListDto>
<section class="locations-1" id="locations">
    <div class="locations py-5">
        <div class="container py-lg-5 py-md-4 py-2">
            <div class="heading text-center mx-auto">
                <h6 class="title-small">En Yeni Mülkler</h6>
                <h3 class="title-big">Son Eklenen İlanlar</h3>
            </div>
            @if (Model is null || Model.Count==0)
            {
                <div class="row pt-md-5 pt-4">
                        <div class="col-lg-4 col-md-6">                        
                                <div class="box16">                                
                                    <img class="img-fluid" src="~/EstateTemp/assets/images/p1.jpg" alt="">
                                    <div class="box-content">
                                        <span class="post">@TempData["ErrorMessage"]</span>
                                    </div>
                                </div>                          
                        </div>
                </div>
            }
            else
            {
                <div class="row pt-md-5 pt-4">
                    @foreach (var estate in Model)
                    {
                        <div class="col-lg-4 col-md-6">
                            <a href="property-single.html">
                                <div class="box16">
                                    <div class="rentext-listing-category"><span>@estate.type</span></div>
                                    <img class="img-fluid" src="~/EstateTemp/assets/images/p1.jpg" alt="">
                                    <div class="box-content">
                                        <h3 class="title">₺@estate.price</h3>
                                        <span class="post">@estate.title</span>
                                    </div>
                                </div>
                            </a>
                        </div>
                    }
                </div>
            }
            
        </div>
    </div>
</section>